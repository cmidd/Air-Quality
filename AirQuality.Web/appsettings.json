{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenAq": {
    "BaseAddress": "https://docs.openaq.org/",
    "Endpoints": {
      "Locations": "v2/locations",
      "Cities": "v2/cities"
    }
  },
  "Caching": {
    "CitiesListKey": "CitiesList",
    "CitiesListExpiration": "86400",
    "SearchHistoryKey": "SearchHistory",
    "SearchHistoryExpiration": "86400"
  },
  "AllowedHosts": "*"
}
